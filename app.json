{
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-8017092196734683~7630132751"
  },
  "expo": {
    "name": "swipewatch",
    "slug": "swipewatch", 
    "version": "1.0.5",
    "orientation": "default",
    "icon": "./assets/images/logo.png",
    "scheme": "swipewatch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.swipewatch.app",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#0D0D0D"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.INTERNET",
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-screen-orientation",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0D0D0D",
          "image": "./assets/images/logo.png",
          "resizeMode": "contain"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8017092196734683~7630132751",
          "iosAppId": "ca-app-pub-8017092196734683~7630132751"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "46ca1d72-7bf9-4687-bc34-3786d0831ef1"
      }
    }
  }
}
